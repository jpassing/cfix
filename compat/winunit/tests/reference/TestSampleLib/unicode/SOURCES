BSCMAKE_FLAGS=$(BSCMAKE_FLAGS) /n

#
# N.B. The WinUnit code requires /W3.
#
MSC_WARNING_LEVEL=/W3

INCLUDES=..\..\..\..\include;..\..\..\..\..\..\include

!if "$(TARGET_DIRECTORY)"=="i386"
USER_C_FLAGS=/D_UNICODE /DUNICODE /wd4800 /analyze /EHa
LINKER_FLAGS=/nxcompat /dynamicbase /SafeSEH
!else
USER_C_FLAGS=/D_UNICODE /DUNICODE /wd4800 /EHa
LINKER_FLAGS=/nxcompat /dynamicbase
!endif

!if "$(DDKBUILDENV)"=="chk"
DEBUG_CRTS=1
C_DEFINES = $(C_DEFINES) /D_DEBUG
!endif

USE_LIBCMT=1
USE_STL=1

TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib \
		   ..\..\..\..\..\..\bin\$(DDKBUILDENV)\$(TARGET_DIRECTORY)\wusamplelibw.lib \
		   ..\..\..\..\..\..\bin\$(DDKBUILDENV)\$(TARGET_DIRECTORY)\cfix.lib

TARGETNAME=testwusamplelibw
TARGETPATH=..\..\..\..\..\..\bin\$(DDKBUILDENV)
TARGETTYPE=DYNLINK

