#BROWSER_INFO=1
BSCMAKE_FLAGS=$(BSCMAKE_FLAGS) /n

MSC_WARNING_LEVEL=/W4 /WX /Wp64

INCLUDES=..\include

USER_C_FLAGS=/D_UNICODE /DUNICODE

UMTYPE=console
UMENTRY=wmain
USE_MSVCRT=1

TARGETLIBS=$(MAKEDIR)\..\bin\$(DDKBUILDENV)\$(TARGET_DIRECTORY)\jpht.lib

TARGETNAME=jphttest
TARGETPATH=..\bin\$(DDKBUILDENV)
TARGETTYPE=PROGRAM
SOURCES=test.c